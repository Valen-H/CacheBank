{
	"plugins": [
		"plugins/markdown",
		"plugins/summarize",
		"plugins/typescript"
	],
	"sourceType": "module",
	"recurseDepth": 10,
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": [ "jsdoc", "closure" ]
	},
	"source": {
		"includePattern": ".+\\.(t|j)s(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./doc/",
		"recurse": true,
		"verbose": true,
		"readme": "README.md"
	}
}
